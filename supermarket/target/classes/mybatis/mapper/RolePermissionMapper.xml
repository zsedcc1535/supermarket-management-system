<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Ambition.mapper.RolePermissionMapper">

    <insert id="insertRolePermissio" parameterType="com.Ambition.pojo.RolePermission">
        insert into supermarket.role_permission(roleId, permissionId)  VALUES (#{roleId},#{permissionId});
    </insert>

    <delete id="deleteRolePermissio" >
        delete from supermarket.role_permission where roleId = #{roleId}
    </delete>

</mapper>