<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Ambition.mapper.LoginRecordMapper">

    <insert id="addLoginRecord" parameterType="com.Ambition.pojo.LoginRecord">
       insert into supermarket.login_record(usercode, username, roleName,loginTime)
       values(#{usercode},#{username},#{roleName},#{loginTime})
    </insert>


</mapper>